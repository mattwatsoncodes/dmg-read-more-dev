{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "dmg/read-more",
	"version": "0.1.0",
	"title": "DMG Read More",
	"category": "widgets",
	"icon": "search",
	"description": "Search for posts by ID or keyword and insert a 'Read More' link to them.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "dmg-read-more",
	"editorScript": "file:./index.js",
	"attributes": {
		"selectedPost": {
			"type": "object",
			"default": {
				"id": null,
				"title": "",
				"link": ""
			}
		}
	},
	"editorStyle": "file:./index.css"
}
